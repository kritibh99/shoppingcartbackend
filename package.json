{
  "name": "shoppingapp",
  "version": "0.0.1",
  "description": "shoppingapp using lb4",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"*/.ts\" \"*/.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/*/.js && npm run posttest",
    "docker:build": "docker build -t shoppingapp .",
    "docker:run": "docker run -p 3000:3000 -d shoppingapp",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "db:migrate": "npm-run-all --parallel db:migrate:*",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "kritibh99 <kriti.bhargava@sourcefuse.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^9.0.0",
    "@loopback/authentication-jwt": "^0.12.0",
    "@loopback/authorization": "^0.12.3",
    "@loopback/boot": "^5.0.0",
    "@loopback/core": "^4.0.0",
    "@loopback/repository": "^5.0.0",
    "@loopback/rest": "^12.0.0",
    "@loopback/rest-explorer": "^5.0.0",
    "@loopback/security": "^0.8.2",
    "@loopback/service-proxy": "^5.0.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-postgresql": "^5.5.0",
    "loopback4-authentication": "^6.1.5",
    "loopback4-authorization": "^5.0.5",
    "nodemailer": "^6.7.5",
    "nyc": "^15.1.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.0",
    "@loopback/eslint-config": "^13.0.0",
    "@loopback/testlab": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^12.20.50",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.14.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.6.4"
  }
}
